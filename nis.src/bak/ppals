alias	m	more
alias	c	clear
alias	del	'rm -i'
alias	so	source
alias	socsh	'source ~mhsatto/.cshrc '

# FILE LISTING
alias	show	'pwd; ls -ot |more '
alias	la	'ls -A '
alias	lal	'ls -Ao \!* |more '
alias	ll	'ls -o \!* |more '
alias	cll	'clear; ll '
alias	llg	'ls -l \!* |more '
alias	lt	'ls -ot \!* |more '
alias	clt	'clear; lt '
alias	lat	'ls -oAt \!* |more '
alias	lg	'ls -dot \!* |more '
alias	le	'ls -ot *.\!* |more '
alias	lsd	'ls -o | grep ^dr |more '
alias	lsz	'ls -o | sort -r -n +4 |more '
alias	lct	'echo `\ls -A |wc -w` files/folders '
alias	lscc	'ls -ot *.cc *.cpp *.C *.c *.cxx |grep -v ',' |more '
alias	lsh	'ls -ot *.h *.hpp *.hh *.hxx |grep -v ',' |more '
alias	lsrc	'lscc > /tmp/mhslsrctemp; lsh >> /tmp/mhslsrctemp; more /tmp/mhslsrctemp |sort -f +8; rm /tmp/mhslsrctemp '
alias	lsmk	'date; ls -ot | egrep "[Mm]ake" '
alias	lso	'date; ls -ot *.o *.obj |more '
alias	lsx	'date; ls -ot | grep -e -rwx |more '

