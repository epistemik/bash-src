# FILE LISTING
alias	la	'ls -a'
alias	lal	'ls -al \!* |more '
alias	ll	'ls -l \!* |more '
alias	lt	'ls -lt \!* |more '
alias	lg	'ls -dlt \!* |more '
alias	le	'lt *.\!* |more '
alias	lsd	'll | grep ^dr |more '
alias	lsz	'll | sort -r -n +4 |more '
alias	lct	'\ls |wc -w '
alias	lscc	'lt *.cc *.cpp *.C |grep -v ',' |more '
alias	lsh	'lt *.h *.hpp *.hh |grep -v ',' |more '
alias	lsrc	'lscc > .lsrctemp; lsh >> .lsrctemp; m .lsrctemp |sort +8 |grep -v ','; rm .lsrctemp'
alias	lso	'date; lt *.o *.obj |more '
alias	lsx	'date; lt | grep -e -rwx |more '
